			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let jsonData = pm.response.json();\r",
							"let salary_user = jsonData.salary;\r",
							"let req = pm.request.url.query.toObject();\r",
							"let req_salary = req.salary;\r",
							"let i=0;\r",
							"while(i<salary_user.length){ \r",
							"    postman.setNextRequest(\"object_info_4\")\r",
							"    pm.test(`Test  salary[${i}]`, function () {   \r",
							"        pm.expect(String(salary_user[i])).to.eql(String(req_salary*(i+1)));   \r",
							"\r",
							"    });\r",
							"    i++;\r",
							"}\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
